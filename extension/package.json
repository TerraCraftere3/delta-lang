{
  "name": "deltalang",
  "displayName": "DeltaLang",
  "description": "Language support for the Delta Compiler",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "delta",
        "aliases": [
          "Delta",
          "delta"
        ],
        "extensions": [
          ".dlt",
          ".delta"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "delta",
        "scopeName": "source.delta",
        "path": "./syntaxes/delta.tmLanguage.json"
      }
    ]
  }
}